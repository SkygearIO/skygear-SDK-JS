  "scripts": {
    "doc": "esdoc -c esdoc.json",
    "dev": "gulp --deploy-env dev dev",
    "minify": "gulp --deploy-env production minify",
    "postinstall": "npm run lerna-bootstrap && npm run npm-install-lock-only",
    "prepare": "npm run build-production",
    "lerna-bootstrap": "lerna bootstrap",
    "build-production": "gulp --deploy-env production babel && gulp --deploy-env production minify",
    "test": "gulp --deploy-env dev",
    "lerna": "lerna",
    "example": "gulp --deploy-env dev example",
    "npm-install": "lerna exec -- npm install",
    "npm-install-lock-only": "lerna exec -- npm install --package-lock-only",
    "audit": "lerna exec -- npm audit"
  },
  "bin": {
    "skygear-node": "./bin.js",
    "skygear-asset": "./asset.js"
  },
  "license": "Apache-2.0",
  "dependencies": {},
  "bugs": {
    "url": "https://github.com/SkygearIO/skygear-SDK-JS/issues"
